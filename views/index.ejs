<!DOCTYPE html>
<html>
  <head>
    <% include partial_head_resource %>

    <!-- canvas styles -->
    <link
      href="stylesheets/impact_canvas.css"
      rel="stylesheet"
      type="text/css"
    />
    <!-- load game -->
    <script type="text/javascript" src="game.min.js"></script>
  </head>
  <body>
    <% include partial_menu %>

    <!-- Page Heading/Breadcrumbs -->
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header">
          <div id="ad_space" alt="Google ads">
            <script
              async
              src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"
            ></script>
            <!-- tower defense game -->
            <ins
              class="adsbygoogle"
              style="display: block"
              data-ad-client="ca-pub-1275631325618401"
              data-ad-slot="9024150973"
              data-ad-format="auto"
              data-full-width-responsive="true"
            ></ins>
            <script>
              (adsbygoogle = window.adsbygoogle || []).push({});
            </script>
          </div>
        </h1>
      </div>
    </div>
    <!-- /.row -->

    <div class="row">
      <div id="game">
        <canvas id="canvas"
          >live node.js server, impact.js html5 framework, homemade tower
          defense javascript game</canvas
        >
      </div>
    </div>
    <!-- /.row -->

    <% include partial_footer %>
  </body>
</html>
